/*客户收货地址css样式*/
.list-block-address.media-list .item-inner-right{
    display: block;
    padding-top: 0.4rem;
    padding-bottom: 0.45rem;
    -webkit-align-self: stretch;
    align-self: stretch;
    text-align: right;
    padding-right: 1rem;
}
.list-block-address {
    margin: 0.75rem 0;
    font-size: 0.85rem;
}
.list-block-address .item-title.checkbox{
	width: 100%;
    -webkit-flex-shrink: 0;
    -ms-flex: 0 0 auto;
    -webkit-flex-shrink: 0;
    flex-shrink: 0;
    margin: 4px 0;
}
.list-block-address .item-title.label{
	width: 40%;
}
.list-block-address ul {
    background: #ffffff;
    list-style: none;
    padding: 0;
    margin: 0;
    position: relative;
}
.item-inner-right-oper{
	display: block;
    padding-top: 0.4rem;
    padding-bottom: 0.45rem;
    -webkit-align-self: stretch;
    align-self: stretch;
    text-align: right;
      padding-left: 4rem;
}

.list-block-address .item-content {
    box-sizing: border-box;
    padding-left: 0.75rem;
    height: 1.8rem;
    min-height: 1.8rem;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
    justify-content: space-between;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
}
.list-block-address .item-subtitle {
    font-size: 0.85rem;
    position: relative;
    white-space: nowrap;
    max-width: 100%;
}
.list-block-address.media-list .item-inner {
    display: block;
    padding-top: 0.5rem;
    padding-bottom: 0.45rem;
    -webkit-align-self: stretch;
    align-self: stretch;
}
.bottom-bar{
    background-color: white;
    bottom: 0;
    width: 100%;
    height: 2.5rem;
    padding: 0;
    table-layout: fixed;
    position: absolute;
    right: 0;
    left: 0;
    z-index: 10;
    height: 2.2rem;
    padding-right: 0.5rem;
    padding-left: 0.5rem;
}

